enable_plugin networking-cisco https://git.openstack.org/openstack/networking-cisco
enable_service net-cisco
ML2_VLAN_RANGES=ucsmnet:{{claimed_vlan_range}}
OVS_VLAN_RANGES=ucsmnet:{{claimed_vlan_range}}
